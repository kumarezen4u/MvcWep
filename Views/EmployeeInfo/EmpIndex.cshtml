@model List<WebMvc.Models.EmployeeInfoModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EmpIndex</title>
</head>
<body>
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <table>
                <thead>
                    <tr>
                        <td> Employee ID</td>
                        <td> Name</td>
                        <td> Gender</td>
                        <td> City</td>
                        <td> Department</td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>@item.EmployeeID </td>
                        <td>@item.Name </td>
                        <td>@item.Gender </td>
                        <td>@item.City </td>
                        <td>@item.Department </td>
                    </tr>

                    }
                </tbody>
            </table>

        </div>
        <div class="col-md-3"></div>
    </div>
</body>
</html>
